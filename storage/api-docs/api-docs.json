            "get": {
                "summary": "Listing products",
                "operationId": "d60f6532d730ced60e88ef335d823570",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "products": {
                                        "type": "array",
                                        "items": {
                                            "properties": {
                                                "title": {
                                                    "type": "string",
                                                    "maxLength": 50,
                                                    "minLength": 2
                                                },
                                                "description": {
                                                    "type": "string",
                                                    "maxLength": 200,
                                                    "minLength": 10
                                                },
                                                "price": {
                                                    "type": "number",
                                                    "format": "float"
                                                },
                                                "created_at": {
                                                    "type": "time"
                                                },
                                                "updated_at": {
                                                    "type": "time"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "id": 1,
                                    "title": "Самса",
                                    "description": "Очень вкусная, с курицей",
                                    "price": "219.99",
                                    "created_at": "2023-07-06T08:27:30.000000Z",
                                    "updated_at": "2023-07-06T09:45:07.000000Z"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
